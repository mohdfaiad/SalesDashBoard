.accordion  
{
    font-size:8.5px;
    font-family:Tahoma;
    /*background:url(../images/Accordian/item_bg.gif	);*/
}
.accordion .header 
{
    background-image:url('images/menu_off.png'); background-repeat: repeat-x; cursor: pointer;
    height: 30px;
    margin-bottom: 2px;
   
}
.header{border-top: 1px solid #a59fdf;}
.accordion .header DIV
{
    cursor:pointer;
    padding-left:40px;
    background-color:Transparent;
    background-position:center left;
    background-repeat:no-repeat;
    
}
.accordion .header SPAN
{
    cursor:pointer;
    font-weight:bold;
    display:block;
    padding-top:8px;
    font-size:11px;
     color:Black;
    
}        
.accordion .header:hover
{
    background-color:Transparent;
    background-image:url('images/menu_on.png'); background-repeat: repeat-x; cursor: pointer;
    border-bottom:none;
}                
.accordion UL        
{
    padding:0;
   /* margin:5px 5px 5px 15px;*/
   margin: 0px;
    list-style-type:none;
}
.accordion LI
{
    background-repeat: no-repeat;
    vertical-align:middle;
    padding:5px 5px 5px 30px;
    cursor:hand;
    margin-bottom: 3px;
    border-top:0px
}
.accordion LI a
{
    background-color:Transparent;
    background-repeat:no-repeat;
    background-position:5px center;
    vertical-align:middle;
    padding:5px 5px 5px 30px;
    color: Black;
    cursor:hand;
    margin-bottom: 3px;
}

.accordionselected
{
    color:#f09e24;
    border-bottom: 1px solid #A3A3A3;
    font-weight:bold;
}