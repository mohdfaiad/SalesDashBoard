/* title Text */
.titletext{font-family: Calibri; color: #4b4494; text-align: left; font-size: 20px; text-transform: uppercase; padding-top: 27px; font-weight: bold; cursor: default; background-image: url('images/inspirehrm_logo.png'); background-repeat: no-repeat; background-position: right center; padding-right: 15px;}
/* Top Menu */

.subhead
{
	color: #4b4494; font-size:15px ;  text-align: center; font-weight:bold ;
}
.topmenu{
	float: left; width: auto
}
.topmenu ul{
	width: auto; margin-right: 55px;
}

.topmenu li{
	float: left; list-style-type: none; display: block; margin-right: 5px;
}
.topmenu li a{
	display: block; background-image: url('images/tab_off.png'); background-repeat: no-repeat; width: 114px; height: 55px; text-decoration: none
}
.topmenu li a:hover{
	display: block; background-image: url('images/tab_on.png'); background-repeat: no-repeat; width: 114px; height: 55px;
}
.topmenu li a.current_tab{
	display: block; background-image: url('images/tab_on.png'); background-repeat: no-repeat; width: 114px; height: 55px;
}
.topmenu li a span.dashboardhome{
	background: url('images/icon_dashboardhome.png'); background-repeat: no-repeat;  background-position: center 10px; padding-top: 36px;
	color: #000; font-weight: normal; width: 114px; display: block; text-align: center; 
}
.topmenu li a span.systemsetup{
	background: url('images/icon_systemsetup.png'); background-repeat: no-repeat;  background-position: center 10px; padding-top: 36px;
	color: #000; font-weight: normal; width: 114px; display: block; text-align: center; 
}
.topmenu li a span.jobpostings{
	background: url('images/icon_jobpostings.png'); background-repeat: no-repeat;  background-position: center 10px; padding-top: 36px;
	color: #000; font-weight: normal; width: 114px; display: block; text-align: center; 
}
.topmenu li a span.candidates{
	background: url('images/icon_candidates.png'); background-repeat: no-repeat;  background-position: center 10px; padding-top: 36px;
	color: #000; font-weight: normal; width: 114px; display: block; text-align: center; 
}
.topmenu li a span.employees{
	background: url('images/icon_employees.png'); background-repeat: no-repeat;  background-position: center 10px; padding-top: 36px;
	color: #000; font-weight: normal; width: 114px; display: block; text-align: center; 
}
.topmenu li a span.reports{
	background: url('images/icon_reports.png'); background-repeat: no-repeat;  background-position: center 10px; padding-top: 36px;
	color: #000; font-weight: normal; width: 114px; display: block; text-align: center; 
}

/* Seperators */
.topbar{
	background-image: url('images/top_bar.png'); background-repeat: repeat-x; height: 7px;
}
.leftbar{
	background-image: url('images/left_border.png'); background-repeat: repeat-y; width: 7px; height: 600px; 
}
.rightbar{
	background-image: url('images/right_border.png'); background-repeat: repeat-y; width: 12px; height: 600px;
}

/* Left Menu & Right Panel */
.leftmenu{
	width: 250px;
	height: 100%;
	background-color: #d6d4f5;
}
#accordion{margin: 0px;}
.rightpanel{
	width: 200px;
	padding: 5px 10px;
}
.rightpanel_3column{
	width: 38%;
	padding: 5px 10px;
}

/* Content Area */
.contentarea{
	padding: 5px 10px;
}

/* Paging */
.paging{
	color: #f05732; border-bottom: 1px solid #d2e1ea; padding-bottom: 5px; font-size: 13px; font-weight: bold
}

.paging label{
	background: url('images/icon_arrow.png'); background-repeat: no-repeat; background-position: left 2px; color: #000; margin: 0px 10px; 
	padding-left: 17px;
}
.paging label a{
	color: #000; text-decoration: none; font-weight: normal
}
.paging label a:hover{
	color: #555; text-decoration: underline
}

/* Titles & labels */
.title{color: #ed9f59; padding: 10px 13px; font-size: 15px; font-weight: bold; border-bottom: 1px solid #999999}
.label{background: #c9e7f9; border-top: 0px solid #68b9ed; border-bottom: 0px solid #68b9ed; padding: 3px; margin-bottom: 5px; font-weight: bold;}


/* footer */

.footer 
{
	background: url('images/footer_logo.png');
         border-top: 10px solid #4b4494;
            width: 100%;
            height: 44px;
            background-repeat: no-repeat;
            background-position: right bottom;
            position: relative;

            }

/*Progress bar*/

#progressBackgroundFilter

{

position: fixed;

top: 0px;

bottom: 0px;

left: 0px;

right: 0px;

overflow: hidden;

padding: 0;

margin: 0;

background-color: #022;

filter: alpha(opacity=50);

opacity: 0.5;

z-index: 1000;

}

#processMessage

{

position: fixed;

top: 30%;

left: 41%;

padding: 10px;

width: 200px;

z-index: 1001;

font-family: Sans-Serif;

color: gray;

background-color: #fff;

border: solid 2px #000;

text-align: center;

}




