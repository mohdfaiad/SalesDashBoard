html, body{}
body, p, ul, li, ol{margin: 0px; padding: 0px;}
body{line-height: 18px; background: #f6f6fa url('images/page_bg.png') repeat-y left top;
}
body,form, input, select{
	font-size: 11px; font-family: Arial, Helvetica, sans-serif;
}

/* Form */
fieldset{
	margin: 0px; margin-top: 10px; border: 2px solid #fff; min-height: 100px;  border-radius:20px;	
}

legend{
    padding: 7px 5px; font-size: 12px; font-weight: bold; color: #000; margin-left: 15px; cursor: default;
    width: 251px;
}
input[type=text], input[type=password]{
background-image: url('images/textbox_bg.png'); background-repeat: repeat-x; border: 1px solid #e1dada;
padding: 4px 5px;
}
select{
	background-color: #f0f8fe;
	border: 1px solid #e1dada;
	min-width: 90px;
	padding: 3px;
    margin-top: 0px;
    text-align: left;
}
textarea{background-color: #f0f8fe; border: 1px solid #e1dada; padding: 3px; min-height: 50px; font-family: Calibri}

input[type=button], input[type=submit]{
	background-image: url('images/btn_bg.png'); border: 1px solid #6799c6; padding: 5px 10px; color: #fff; font-weight: bold;
	 -moz-border-radius: 4px;
  border-radius: 4px;
  cursor: pointer

}
input[type=button]:hover, input[type=submit]:hover{
	background-image: url('images/btn_bg_over.png'); border: 1px solid #6799c6; padding: 5px 10px; color: #fff; font-weight: bold;
	 -moz-border-radius: 4px;
  border-radius: 4px;
 cursor: pointer
}

input[disabled="disabled"]{opacity:0.4; 

filter:alpha(opacity=40); /* For IE8 and earlier */ 

cursor: default;

}

input[disabled="disabled"]:hover{opacity:0.4; 

filter:alpha(opacity=40); /* For IE8 and earlier */ 

cursor: default;

}

/*empty lable*/
.emptylabel
{
    background-color: #ccc; border: 1px solid #666; color: Red; font-weight: bold; text-align: center; vertical-align: middle;
}

/* Grid */
.grayreport{font-size:11px
}
.grayreport th{background-image: url('images/menu_off.png'); background-repeat: repeat-x; padding: 4px 6px;color: black; text-align: left; border: 0px;font-size:11px}
.grayreport tr{background-color: #f6f6fa;}
.grayreport tr.altern{background-color: #d6d4f5;}
.grayreport tr.selected{background-color: #fad07f;}
.grayreport tr:hover{background-color: #a8c8eb;}

.grayreport tr td{padding: 4px 6px; color: #000000; border: 0px;  text-align: left}
.grayreport tr.altern td{padding: 4px 6px; color: #000000; border: 0px; text-align: left}

.grayreport tr td a{color: #006699; text-decoration: none}
.grayreport tr td a:hover{color: #006699; text-decoration: underline}

.grayreport .footer td{text-align: right; background-color: #9ab9e5;}
.grayreport .footer td table tr td{padding: 0px 6px; text-align: right; color: #ffffff}
.grayreport .footer td table tr td a{text-align: right; color: #ffffff; text-decoration: none; padding:: 3px;
text-decoration: none; padding: 3px;}

.grayreport tr.empty td{background-color: #ccc; border: 1px solid #666; color: Red; font-weight: bold; text-align: center; vertical-align: middle;}
/* Grid */

.sideheading{color: #618eb7; font-weight: bold; font-size: 12px; padding: 5px; text-align: left; text-transform: uppercase;}
.sideheading_orange{color: orange; font-weight: bold; font-size: 12px; padding: 0px; text-align: left; text-transform: uppercase;}
.sideheading a{color: #618eb7; font-weight: bold; font-size: 12px; text-decoration: none; text-transform: none; padding-left: 38px; color: #000}
.sideheading a:hover{text-decoration: underline}
